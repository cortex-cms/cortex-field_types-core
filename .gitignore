# Ignore bundler config.
/.bundle
/vendor/bundle
/vendor/assets

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Debug
.byebug_history

# OS Files
.DS_Store
._.DS_Store
.directory


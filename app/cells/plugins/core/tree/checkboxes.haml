= render_label
= render_tooltip unless @options[:tooltip].nil?
- @options[:metadata]['head'].each do |checkbox_key|
  = @options[:form].hidden_field :field_id, value: checkbox_key
  = cell(Plugins::Core::CheckboxCell, nil, form: @options[:form], node_key: checkbox_key, tree_fields: @options[:metadata]['tree_fields'], data: data).(:checkbox)
